!!!
html
  head
    title= 'The Rock - Ames, IA'
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel='stylesheet', href='/css/bootstrap.min.css')
    link(rel='stylesheet', href='/css/style.css')
    script(src='/js/jquery.js')
    script(src='/js/bootstrap.min.js')
  body
    #wrap
      .navbar.navbar-default.navbar-inverse
        .container
          .navbar-header
            a(data-toggle='collapse',data-target='.navbar-collapse').btn.navbar-toggle
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a(href='/').navbar-brand The Rock
          .navbar-collapse.collapse
            ul.nav.navbar-nav
              - var obj = { 'index':'Home', 'about':'About', 'contact':'Contact', 'blog':'Blog' }
                - each val, key in obj
                  - if (id == key)
                    li.active
                      a(href='#{key}') #{val}
                  - else            
                    li
                      a(href='#{key}') #{val}
      .container
        .row
          .col-md-8
            block content
          .col-md-4
            include includes/calendar
    #footer
      .container
        include includes/about

